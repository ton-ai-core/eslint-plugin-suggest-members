{
  "name": "eslint-plugin-suggest-members",
  "version": "1.2.3",
  "description": "ESLint plugin suggesting potential corrections when accessing non-existent object members in TypeScript.",
  "main": "dist/index.js",
  "files": [
    "dist/",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint --ext .ts .",
    "lint:default": "eslint --ext .ts .",
    "example:build": "tsc -p example/tsconfig.json",
    "example:lint": "eslint --ext .ts example/src/ -c example/eslint.config.js",
    "start": "npm run build && npm run example:lint",
    "test:typos": "eslint --ext .ts example/src/example1-typos.ts -c example/eslint.config.js",
    "test:similarity": "eslint --ext .ts example/src/example2-similarity.ts -c example/eslint.config.js",
    "test:edge-cases": "eslint --ext .ts example/src/example3-edge-cases.ts -c example/eslint.config.js",
    "test:jaro-winkler": "eslint --ext .ts example/src/example4-jaro-winkler.ts -c example/eslint.config.js",
    "test:corner-cases": "eslint --ext .ts example/src/example5-corner-cases.ts -c example/eslint.config.js",
    "test": "npm run build && npm run test:typos && npm run test:similarity && npm run test:edge-cases && npm run test:jaro-winkler && npm run test:corner-cases",
    "lint:formatter": "eslint --ext .ts example/src/ -c example/eslint.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ton-ai-core/eslint-plugin-suggest-members.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "typescript",
    "suggestmembers",
    "eslint-plugin",
    "code-quality",
    "typo-detection"
  ],
  "author": "TON Foundation",
  "license": "MIT",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/ton-ai-core/eslint-plugin-suggest-members/issues"
  },
  "homepage": "https://github.com/ton-ai-core/eslint-plugin-suggest-members#readme",
  "peerDependencies": {
    "@typescript-eslint/parser": ">=5.0.0",
    "eslint": ">=7.0.0",
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@ton/core": "^0.60.1",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "@typescript-eslint/rule-tester": "^8.29.0",
    "@typescript-eslint/utils": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-formatter-codeframe": "^7.32.1",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.1",
    "typescript": "^5.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
